<h6>Dashboard Page</h6>
<h3>Welcome {{name}}</h3>
<p>Here are your stories</p>
{{#if stories}}
  <table class="striped">
    <thead>
      <tr>
        <th>Title</th>
        <th>Date</th>
        <th>Status</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {{#each stories}}
        <tr>
          <td><a href="/stories/{{_id}}"></a>{{title}}</td>
          <td>{{formatDate createdAt 'MMMM Do YYYY, h:mm:ss:s'}}</td>
          <td><span class="dash-status"></span>{{status}}</td>
          <td></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>You have not created any stories</p>
{{/if}}

